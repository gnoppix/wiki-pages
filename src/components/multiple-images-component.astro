---
import MultipleImageComponent from './multi-image-component';
import getImgPath from './images';

interface Props {
  images: object[];
}

const { images } = Astro.props;

let requestedImages = [];
for (const image of images) {
  requestedImages.push(await getImgPath(image));
}
---

<MultipleImageComponent client:visible images={requestedImages} />
